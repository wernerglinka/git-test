<div class="header-wrapper js-header">
  <header>
    <div class="container">
      <div class="brand">
        <a class="js-home-link home-link" href="/">
          <span class="sr-only">Go home</span>
          {% include "../icons/logo-bug.njk" %}
        </a>
      </div>
      <nav class="nav">
        <ul class="main-menu">
          {% for item in refData.navigation %}
            <li>
              <a class="{{ item.label | toLower }}" href="{{ item.url }}">{{ item.label }}</a>
            </li>
          {% endfor %}
        </ul>
      </div>
    </header>
  </div>