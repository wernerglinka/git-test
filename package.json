  "dependencies": {
    "@metalsmith/drafts": "^1.3.0",
    "@metalsmith/js-bundle": "^0.8.0",
    "@metalsmith/layouts": "^2.7.0",
    "@metalsmith/markdown": "^1.10.0",
    "@metalsmith/metadata": "^0.3.0",
    "@metalsmith/permalinks": "^3.0.1",
