{
  "name": "start-here",
  "version": "0.0.2",
  "description": "The Metalsmith First starter kit",
  "type": "module",
  "keywords": [
    "metalsmith",
    "starter",
    "metalsmith first"
  ],
  "author": "werner@glinka.co",
